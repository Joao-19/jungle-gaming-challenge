{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "modulePaths": ["<rootDir>/../"],
  "globals": {
    "ts-jest": {
      "tsconfig": "<rootDir>/../tsconfig.json"
    }
  }
}


